global
const
type
proc
procedure main;
dcl
	x : real;
	sum : real;
	n : int;
	term : real;
	i : int;
	fact : real;

begin
	n := 0;
	x := 0.523598775;
	sum := 0;
	while(n < 10)
		do;
			term := -1;
			if((n / 2) = (n / 2.0)) then
				do;
					term := +1;
				end;
			i := 0;
			while(i < 2*n + 1)
				do;
					term := term * x;
					i := i + 1;
				end;
			fact := 1;

			i := 1;
			while(i <= 2*n + 1)
				do;
					fact := fact * i;
					i := i + 1;
				end;
			term := term / fact;
			sum := sum + term;
			n := n + 1;

		end;
	writeln(sum);

end.
