global
const
type
    myArr : array [8] of real;
dcl
    test : myArr;
{proc
factorial(n : int, var i : int) : int;}


procedure main();
dcl
    n : int;
    x : int;
    asdf : myArr;
begin
    readln(asdf[0], asdf[1], asdf[2]);
    writeln(asdf[0], asdf[1], asdf[2]);
    readln(n);
    writeln("n! = ", factorial(n, x));
    writeln("x = ", x);
end

procedure factorial(n : int, var i : int) : int;
dcl
begin
    i := i + 1;
    if(n = 0) then
        return 1;
    return n*factorial(n-1, i);
end
.
